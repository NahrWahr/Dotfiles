!! URxvt Appearance
urxvt.font: xft:DejaVu Sans Mono:pixelsize=18
urxvt.boldFont: xft:DejaVu Sans Mono:pixelsize=18:weight=bold
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt*geometry: +116+33
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 32
URxvt.fading: 20
URxvt.iso14755: false
URxvt.borderLess: true


!! URxvt Extensions

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,matcher,tabbed,selection-to-clipboard,pasta

URxvt.keysym.Control-Shift-V:     perl:pasta:paste

URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5

URxvt.url-select.launcher:  /usr/bin/firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste


